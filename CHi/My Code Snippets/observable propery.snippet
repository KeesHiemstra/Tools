					<Default>myVar</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[ObservableCollection<$type$> $property$ { get; set; } = new ObservableCollection<$type$>(); $end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
